    "minify",
    "csso",
    "webpack",
    "optimisation"
  ],
  "main": "./lib/index",
  "engines": {
    "node": "^10.13.0 || ^12.20.0 || ^14.13.0 || >=15.0.0",
    "npm": ">=7.0.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zoobestik/csso-webpack-plugin.git"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "npm run build && jest",
    "ci-test": "jest",
