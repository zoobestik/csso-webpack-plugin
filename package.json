    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/eslint-parser": "^7.24.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^27.5.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^6.0.1"
